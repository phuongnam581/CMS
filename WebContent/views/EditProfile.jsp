<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>

 <div class="edit" id="edit">
          <h1>Edit Profile</h1>
          <hr />
          <div class="detail" id="detai-edit">
            <div class="title">
              <p>Profile Form Elements</p>
            </div>
            <form action="">
              <p>First Name</p>
              <input
                type="text"
                name=""
                id=""
                placeholder="Enter the frist name"
                class="input"
              />
              <p>Last Name</p>
              <input
                type="text"
                name=""
                id=""
                placeholder="Enter the last name"
                class="input"
              />
              <p>Email</p>
              <input
                type="email"
                name=""
                id=""
                placeholder="your_email@exmaple.com"
                class="input email"
                readonly
              />
              <p>Phone</p>
              <input
                type="text"
                name=""
                id=""
                placeholder="Enter your phone number"
                class="input"
              />
              <p>Description</p>
              <textarea name="" id="" cols="30" rows="10"></textarea>
              <div class="button-form">
                <input type="button" value="Submmit button" />
                <input type="button" value="Reset button" />
              </div>
            </form>
          </div>
        </div>
   